\begin_inset Formula 
\begin{align*}
F(\boldsymbol{x}) & =\|\boldsymbol{x}\|\\
\frac{\partial F}{\partial x_{n}} & =\frac{x_{n}}{\|\boldsymbol{x}\|}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $F\left(G\left(\boldsymbol{x}\right)\right)=\|\boldsymbol{x}-\boldsymbol{y}\|$
\end_inset

,
\begin_inset Formula $G\left(\boldsymbol{x}\right)=\boldsymbol{x}-\boldsymbol{y}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial F\circ G}{\partial x_{n}}(\boldsymbol{x})=\frac{x_{n}-y_{n}}{\|\boldsymbol{x}-\boldsymbol{y}\|}\frac{\partial G}{\partial x_{n}}(\boldsymbol{x})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $T\left[F\left(G\left(\boldsymbol{x}\right)\right)\right]=\|\boldsymbol{x}-\boldsymbol{y}\|^{3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial T\circ F\circ G}{\partial x_{n}}(\boldsymbol{x})=3*\|\boldsymbol{x}-\boldsymbol{y}\|^{2}\frac{x_{n}-y_{n}}{\|\boldsymbol{x}-\boldsymbol{y}\|}=3*\|\boldsymbol{x}-\boldsymbol{y}\|\left(x_{n}-y_{n}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Minimize & F_{o}\left(\boldsymbol{x}\right)\\
Subject\;to & F_{j}\left(\boldsymbol{x}\right)\leq1\:\left(j=1,...,M\right)\\
 & A_{i}\leq x_{i}\leq B_{i}\:\left(i=1,...,N\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\boldsymbol{x}_{i}=\left[x_{1},x_{2},...,x_{k}\right]^{T}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $Y=F\left(\boldsymbol{x}_{i}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $y=\beta_{0}+\beta_{1}x_{1}+\beta_{2}x_{2}+\beta_{12}x_{1}x_{2}+\beta_{11}x_{1}^{2}+\beta_{22}x_{2}^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $y=\boldsymbol{\beta}\boldsymbol{x}+\epsilon$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{bmatrix}1 & x_{11} & \cdots & x_{1d} & x_{11}x_{12} & \cdots & x_{1\left(d-1\right)}x_{1d} & x_{11}^{2} & \cdots & x_{1d}^{2} & \cdots\\
1 & x_{21} & \cdots & x_{2d} & x_{21}x_{22} & \cdots & x_{2\left(d-1\right)}x_{2d} & x_{21}^{2} & \cdots & x_{2d}^{2} & \cdots\\
\vdots & \vdots & \ddots & \vdots & \vdots & \ddots & \vdots & \vdots & \ddots & \vdots & \cdots\\
1 & x_{n1} & \cdots & x_{nd} & x_{n1}x_{n2} & \cdots & x_{n\left(d-1\right)}x_{nd} & x_{n1}^{2} & \cdots & x_{nd}^{2} & \cdots
\end{bmatrix}\centerdot\begin{bmatrix}\beta_{0}\\
\beta_{1}\\
\vdots\\
\beta_{d}\\
\beta_{12}\\
\vdots\\
\beta_{\left(d-1\right)d}\\
\vdots
\end{bmatrix}=\begin{bmatrix}f\left(\boldsymbol{x}_{1}\right)\\
f\left(\boldsymbol{x}_{2}\right)\\
\vdots\\
f\left(\boldsymbol{x}_{n}\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
f\left(x\right)= & x_{1}+x_{2}+x_{3}\\
g_{1}\left(x\right)= & -1+0.0025\left(x_{4}+x_{6}\right)\leq0\\
g_{2}\left(x\right)= & -1+0.0025\left(x_{5}+x_{7}-x_{4}\right)\leq0\\
g_{3}\left(x\right)= & -1+0.01\left(x_{8}-x_{5}\right)\leq0\\
g_{4}\left(x\right)= & -x_{1}x_{6}+833.33252x_{4}+100x_{1}-83333.333\leq0\\
g_{5}\left(x\right)= & -x_{2}x_{7}+1250x_{5}+x_{2}x_{4}-1250x_{4}\leq0\\
g_{6}\left(x\right)= & -x_{3}x_{8}+1250000+x_{3}x_{5}-2500x_{5}\leq0
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\boldsymbol{V}_{i}^{t+1} & =\omega\boldsymbol{V}_{i}^{t}+\alpha\boldsymbol{\epsilon_{1}}\left(\boldsymbol{pbes}\boldsymbol{t}_{i}^{t}-\boldsymbol{x_{i}^{t}}\right)+\beta\boldsymbol{\epsilon_{2}}(\boldsymbol{gbes}\boldsymbol{t}_{i}^{t}-\boldsymbol{x_{i}^{t}})\\
\boldsymbol{x}_{i}^{t+1} & =\boldsymbol{x}_{i}^{t}+\boldsymbol{V}_{i}^{t+1}\\
\alpha,\beta & \in[0,1)
\end{align*}

\end_inset


\end_layout

